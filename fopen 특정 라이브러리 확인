Interceptor.attach(
    Module.findExportByName(null, "fopen"),
    {
        onEnter(args) {
            var lr = this.context.lr;
            var m = Process.findModuleByAddress(lr);
            if (m && m.name === "libdxbase.so") {
                console.log("[+] fopen from libdxbase.so :", lr);
            }
        }
    }
);