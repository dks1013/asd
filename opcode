const ranges = Process.enumerateRangesSync({
  protection: 'r-x',
  coalesce: true
});

for (const r of ranges) {
  Memory.scan(r.base, r.size, "E8 AD 36 06 00 85 C0", {
    onMatch(addr) {
      console.log("fffff");
      console.log("[HIT] pattern at", addr);
      return 'stop'; // 한 군데만
    },
    onComplete() {}
  });
}